		<InteractionGroup name="WEB_DETAILS" >
		    <InteractionVariable name="HTTPS_ENABLE">1</InteractionVariable> Single digit
		    <InteractionVariable name="WEB_SERVER_IP">192.168.3.41</InteractionVariable>---(WEB Host/IP it might be Integer or char)
			<InteractionVariable name="WEB_SERVER_PORT">7002</InteractionVariable>--4 digit or 3 digit			
			<InteractionVariable name="CONTEXT_NAME">FICHOME</InteractionVariable>---Char			
			<InteractionVariable name="WEBAPP_CONTEXT_PATH">/u01/Oracle/Middleware/Oracle_Home/wlserver</InteractionVariable>---Path
			<InteractionVariable name="WEB_LOCAL_PATH">/u01/OFSAA/FTPSHARE</InteractionVariable>---path
		</InteractionGroup>
		<InteractionGroup name=" Weblogic Setup Details" >
			<InteractionVariable name="WEBLOGIC_DOMAIN_HOME">/u01/Oracle/Middleware/Oracle_Home/user_projects/domains/DEMO_OFSAA_DOMAIN</InteractionVariable>---path
		</InteractionGroup>
		<InteractionGroup name="OFSAAI FTP Details">		       	       
			   <InteractionVariable name="OFSAAI_FTPSHARE_PATH">/u01/OFSAA/FTPSHARE</InteractionVariable>---path		       
