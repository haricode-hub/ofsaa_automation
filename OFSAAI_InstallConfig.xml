<?xml version="1.0" encoding="UTF-8"?>

<UserInteractions>
	<Layer name="GENERAL">		
		<InteractionGroup name="WebServerType" >
			<InteractionVariable name="WEBAPPSERVERTYPE">3</InteractionVariable>---(single digit 1,2,3 isexpected) 1,2,3.
		</InteractionGroup>
		<InteractionGroup name="OFSAA Infrastructure Server Details">			
			<InteractionVariable name="DBSERVER_IP">192.168.3.42</InteractionVariable>---(DB Host/IP it might be Integer or char)
		</InteractionGroup>
		<InteractionGroup name="Database Details" >    					
			<InteractionVariable name="ORACLE_SID/SERVICE_NAME">OFSAAPDB</InteractionVariable>-----(CHAR,Integer and special char)		
